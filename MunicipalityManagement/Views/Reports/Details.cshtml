@model MunicipalityManagement.Models.Report

<h2>Report Details</h2>

<div class="card p-4 shadow-sm">
	<p><strong>Report Type:</strong> @Model.ReportType</p>
	<p><strong>Details:</strong> @Model.Details</p>
	<p><strong>Citizen:</strong> @Model.Citizen?.FullName</p>
	<a href="@Url.Action("Index", "Reports")" class="btn btn-primary">Back</a>
</div>
